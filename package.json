{
  "name": "oldweaver",
  "version": "0.0.1",
  "description": "Saro's profile site",
  "scripts": {
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "start": "npm run dev",
    "dev": "run-s clean eleventy:dev",
    "lint": "eslint src/preact",
    "validate": "run-s lint test-preact",
    "build": "run-s clean eleventy:prod",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "clean": "rimraf dist/",
    "test-preact": "jasmine",
    "e2e:dev": "run-p -r dev e2e-tests",
    "e2e:ci": "run-p -rc dev e2e-tests:ci",
    "e2e-tests": "cypress run --config watchForFileChanges=true",
    "e2e-tests:ci": "cypress run --config watchForFileChanges=false",
    "test-preact:watch": "nodemon -e js --exec 'npm run test-preact'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@testing-library/jasmine-dom": "^1.2.0",
    "clean-css": "^5.1.5",
    "critical": "^4.0.1",
    "cssesc": "^3.0.0",
    "cypress": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-preact": "^1.1.4",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-minifier": "^4.0.0",
    "jasmine": "^3.9.0",
    "jsdom": "^17.0.0",
    "luxon": "^1.26.0",
    "markdown-it": "^12.1.0",
    "markdown-it-anchor": "^8.1.2",
    "memfs": "^3.2.4",
    "mime": "^2.5.2",
    "node-sass": "^6.0.1",
    "svg-sprite": "^1.5.2",
    "webpack": "^5.51.2"
  },
  "dependencies": {
    "@babel/core": "^7.15.4",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/preact": "^2.0.1",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-reader-bar": "^0.2.0",
    "focus-visible": "^5.2.0",
    "htm": "^3.1.0",
    "lodash": "^4.17.21",
    "micromodal": "^0.4.6",
    "milligram": "^1.4.1",
    "mobile-device-detect": "^0.4.3",
    "navigo": "^8.11.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "rimraf": "^3.0.2",
    "tachyons": "^4.12.0"
  }
}
